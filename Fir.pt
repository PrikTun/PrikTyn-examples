i = 0

for (i = 0, i < 12, i = i + 2) {
    spaces = ""
    j = 0
    for (j = 0, j < (11 - i) / 2, j = j + 1) {
        spaces = spaces + " "
    }
    stars = "*"
    for (j = 0, j < i, j = j + 1) {
        stars = stars + "*"
    }
    print spaces + stars + "\n"
}

